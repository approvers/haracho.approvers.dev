{
  "name": "haracho-approvers-dev",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "dev": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "fix": "yarn format && yarn eslint:fix",
    "check": "prettier --check \"./**/*.{ts,tsx,js,md,mdx}\"",
    "format": "prettier --write \"./**/*.{ts,tsx,js,md,mdx}\"",
    "eslint": "eslint \"./**/*.{ts,tsx,js,md,mdx}\"",
    "eslint:fix": "eslint \"./**/*.{ts,tsx,js,md,mdx}\" --fix",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,md}": [
      "yarn fix"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "2.3.1",
    "@docusaurus/preset-classic": "2.3.1",
    "@docusaurus/theme-classic": "^2.3.1",
    "@docusaurus/theme-common": "^2.3.1",
    "@docusaurus/theme-live-codeblock": "^2.3.1",
    "@docusaurus/theme-mermaid": "^2.3.1",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.3.1",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "packageManager": "yarn@3.3.1"
}
